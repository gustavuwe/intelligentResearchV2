generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  email String @unique
  candidate String
  geo_loc String
}

model candidate {
  name String @unique
  votes Int @default(0)
}